{
  "name": "plgg",
  "version": "0.0.19",
  "description": "Pipeline utilities",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.es.js"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs.js"
    }
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "vite build",
    "test": "tsc --noEmit && vitest --run",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "test:watch": "vitest",
    "coverage": "vitest --run --coverage",
    "coverage:watch": "vitest --coverage",
    "publish": "vite build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qmu/plgg.git"
  },
  "author": "qmu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qmu/plgg/issues"
  },
  "homepage": "https://github.com/qmu/plgg#readme",
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
